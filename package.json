{
  "name": "mayrino-jekyll",
  "description": "base on bootstrap-jekyll  a person  mobile first blog web ",
  "version": "1.0.0",
  "version_short": "1.0",
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web",
    "blog",
    "faux fur",
    "textile",
    "fiber faric"
  ],
  "homepage": "https://mayrino.com/",
  "author": "Samon Xie",
  "contributors": [
    "Hangzhou Yuanji, Inc."
  ],
  "scripts": {
    "start": "npm-run-all --parallel watch site-serve",
    "blc": "blc --exclude-external --filter-level 3 --follow --get --ordered --recursive --host-requests 4 --input http://localhost:3000/",
    "http-server": "http-server --silent -p 3000",
    "bundlesize": "bundlesize",
    "check-broken-links": "npm-run-all --parallel --race \"http-server -- _gh_pages/\" blc",
    "css": "npm-run-all css-lint css-compile css-prefix css-minify css-copy",
    "css-copy": "cross-env-shell shx cp -r dist/css/ site/assets/",
    "css-compile": "cross-env-shell node-sass --output-style expanded --source-map true --source-map-contents true --precision 6  site/_sass/mayrino -o dist/css/ && npm run css-copy",
    "css-lint": "npm-run-all --continue-on-error --parallel css-lint-*",
    "css-lint-main": "stylelint \"stylelint \" site/_sass/mayrino/**/*.scss\"  \"site/assets/css/**/*.css\" --cache --cache-location .cache/.stylelintcache",
    "css-lint-vars": "fusv site/_sass/mayrino/ ",
    "css-minify": "cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/mayrino.min.css dist/css/mayrino.css",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "pickup-css": "webpack --config build/webpack.config.js --display-error-details --progress --colors",
    "font-genertor": "webpack --config build/webpack.config.js --display-error-details --progress --colors",
    "js": "npm-run-all js-compile js-minify js-copy",
    "js-copy": "cross-env-shell shx mkdir -p site/dist/ && cross-env-shell shx cp -r dist/js/ site/dist/",
    "js-main": "npm-run-all js-lint js-compile js-minify-main",
    "js-compile": "npm-run-all --parallel js-compile-* --sequential js-copy",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config build/rollup.config.js --sourcemap",
    "js-compile-plugins": "node build/build-plugins.js",
    "js-compile-plugins-coverage": "cross-env NODE_ENV=test node build/build-plugins.js",
    "js-lint": "npm-run-all --continue-on-error --parallel js-lint-*",
    "js-lint-main": "eslint --cache --cache-location .cache/.eslintcache js/src js/tests build/",
    "js-minify": "npm-run-all --parallel js-minify-main js-minify-site",
    "js-minify-main": "npm-run-all js-minify-standalone js-minify-bundle",
    "js-minify-standalone": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/mayrino.js.map,includeSources,url=mayrino.min.js.map\" --output dist/js/mayrino.min.js dist/js/mayrino.js",
    "js-minify-bundle": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/mayrino.bundle.js.map,includeSources,url=mayrino.bundle.min.js.map\" --output dist/js/mayrino.bundle.min.js dist/js/mayrino.bundle.js",
    "js-minify-site": "cross-env-shell uglifyjs --mangle --comments \\\"/^!/\\\" --output site/assets/js/marino.min.js site/assets/js/vendor/bootstrap@4.4.1.min.js js/src/*.js\"",
    "js-test": "npm-run-all js-test-karma* js-test-integration",
    "js-test-karma": "karma start js/tests/karma.conf.js",
    "js-test-karma-old": "cross-env USE_OLD_JQUERY=true npm run js-test-karma",
    "js-test-karma-bundle": "cross-env BUNDLE=true npm run js-test-karma",
    "js-test-karma-bundle-old": "cross-env BUNDLE=true USE_OLD_JQUERY=true npm run js-test-karma",
    "js-test-integration": "rollup --config js/tests/integration/rollup.bundle.js",
    "js-test-cloud": "cross-env BROWSER=true npm run js-test-karma",
    "lint": "npm-run-all --parallel js-lint css-lint",
    "site-compile": "bundle exec jekyll build",
    "site-production": "cross-env JEKYLL_ENV=production npm run site-compile",
    "site-netlify": "cross-env JEKYLL_ENV=netlify npm run site-compile",
    "site-lint": "node build/vnu-jar.js",
    "site-serve": "bundle exec jekyll serve --trace --watch",
    "site-serve-only": "npm run site-serve -- --skip-initial-build --no-watch",
    "update-deps": "ncu -a -x \"cross-env,eslint,find-unused-sass-variables,karma,karma-browserstack-launcher,karma-qunit,nodemon,qunit,rollup,stylelint,stylelint-config-twbs-bootstrap\" && npm update && bundle update && cross-env-shell echo Manually update \\\"site/assets/js/vendor/\\\"",
    "release": "npm-run-all dist release-sri release-zip site-production",
    "release-sri": "node build/generate-sri.js",
    "release-version": "node build/change-version.js",
    "release-zip": "cross-env-shell \"shx rm -rf mayrino-jekyll-$npm_package_version-dist && shx cp -r dist/ mayrino-jekyll-$npm_package_version-dist && zip -r9 mayrino-jekyll-$npm_package_version-dist.zip mayrino-jekyll-$npm_package_version-dist && shx rm -rf mayrino-jekyll-$npm_package_version-dist\"",
    "dist": "npm-run-all --parallel css js",
    "test": "npm-run-all lint dist js-test site-compile site-lint",
    "netlify": "npm-run-all dist release-sri site-netlify",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css-main": "nodemon --watch scss/ --ext scss --exec \"npm run css-main\"",
    "watch-js-main": "nodemon --watch js/src/ --ext js --exec \"npm run js-compile\""
  },
  "style": "dist/css/mayrino-jekyll.css",
  "sass": "scss/mayrino/mayrino.scss",
  "main": "dist/js/mayrino-jekyll",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayrino/mayrino-jekyll.git"
  },
  "bugs": {
    "url": "https://github.com/mayrino/issues"
  },
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "jquery": "1.9.1 - 3",
    "popper.js": "^1.16.0",
    "bootstrap": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "bootstrap": "^4.4.1",
    "broken-link-checker": "^0.7.8",
    "bundlesize": "^0.18.0",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "find-unused-sass-variables": "^0.6.0",
    "font-spider": "^1.3.5",
    "glob": "^7.1.6",
    "hammer-simulator": "0.0.1",
    "http-server": "^0.12.3",
    "ip": "^1.1.5",
    "jquery": "^3.4.1",
    "karma": "^5.0.9",
    "karma-browserstack-launcher": "1.4.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-detect-browsers": "^2.3.3",
    "karma-firefox-launcher": "^1.2.0",
    "karma-qunit": "^3.1.3",
    "karma-sinon": "^1.0.5",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.0",
    "postcss-cli": "^6.1.3",
    "qunit": "2.9.2",
    "rollup": "1.26.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "sinon": "^7.5.0",
    "stylelint": "^9.10.1",
    "stylelint-config-twbs-bootstrap": "^0.5.0",
    "uglify-js": "^3.6.9",
    "vnu-jar": "19.9.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "js/{src,dist}/**/*.{js,map}",
    "scss/**/*.scss"
  ],
  "jspm": {
    "registry": "npm",
    "main": "js/mayrino",
    "directories": {
      "lib": "dist"
    },
    "shim": {
      "js/bootstrap": {
        "deps": [
          "jquery",
          "popper.js",
          "bootstrap"
        ],
        "exports": "$"
      }
    },
    "dependencies": {},
    "peerDependencies": {
      "jquery": "1.9.1 - 3",
      "popper.js": "^1.16.0",
      "bootstrap": "^4.4.1"
    }
  }
}
